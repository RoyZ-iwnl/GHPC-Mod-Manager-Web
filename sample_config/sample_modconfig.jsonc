    "Id": "sample-scripted-mod",
    "Name": {
      "en-US": "Sample Scripted Mod",
      "zh-CN": "示例脚本MOD"
    },
    "Description": {
      "en-US": "A sample mod demonstrating the Scripted install method. Uses a custom batch script to handle installation logic beyond simple file extraction.",
      "zh-CN": "演示脚本安装方式的示例MOD，使用自定义批处理脚本处理超出简单文件解压范围的安装逻辑。"
    },
    "Tags": {
      "sample": { "zh-CN": "示例", "en-US": "Sample" },
      "scripted": { "zh-CN": "脚本安装", "en-US": "Scripted" }
    },
    "SupportedGameVersions": ["20260210"],
    "ReleaseUrl": "https://api.github.com/repos/REPO-OWNER/REPO/releases/latest",
    "TargetFileNameKeyword": ".dll",
    "MainBinaryFileName": "sample.dll",
    "ConfigSectionName": "SampleMod",
    "InstallMethod": "Scripted",
    "InstallScript_Base64": "",
    /*
        脚本参数说明:
    - %1: 已下载文件的完整路径 (如:C:\temp\download_xxx_ATAC.dll)
    - %2: 游戏根目录路径 (如:C:\Games\GHPC)
    - %3: 目标文件名 (如:sample.dll，来自ModConfig.MainBinaryFileName)


      @echo off
      REM Parameters: %1 = Downloaded file path, %2 = Game root directory, %3 = Target filename

      set "DOWNLOAD_FILE=%~1"
      set "GAME_ROOT=%~2"
      set "TARGET_FILE=%~3"
      set "MODS_DIR=%GAME_ROOT%\Mods"

      if not exist "%DOWNLOAD_FILE%" (
          echo ERROR: Downloaded file not found: %DOWNLOAD_FILE%
          exit /b 1
      )

      if not exist "%MODS_DIR%" mkdir "%MODS_DIR%"

      for %%F in ("%DOWNLOAD_FILE%") do set "FILE_EXT=%%~xF"

      if /i "%FILE_EXT%"==".dll" (
          echo Installing DLL: %TARGET_FILE%
          copy "%DOWNLOAD_FILE%" "%MODS_DIR%\%TARGET_FILE%"
      ) else if /i "%FILE_EXT%"==".zip" (
          echo Extracting ZIP to Mods directory
          powershell -Command "Expand-Archive -Path '%DOWNLOAD_FILE%' -DestinationPath '%MODS_DIR%' -Force"
      ) else (
          echo Installing file: %TARGET_FILE%
          copy "%DOWNLOAD_FILE%" "%MODS_DIR%\%TARGET_FILE%"
      )

      echo Installation completed successfully

    */
    "Conflicts": [],
    "Requirements": []
  },
